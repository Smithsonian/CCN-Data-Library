          <measurementScale>
            <ratio>
              <unit>
                <standardUnit>celsius</standardUnit>
              </unit>
              <numericDomain>
                <numberType>real</numberType>
              </numericDomain>
            </ratio>
          </measurementScale>
        </attribute>
        <attribute>
